{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ],
      "env": {}
    },
    "fetch": {
      "command": "npx",
      "args": ["mcp-fetch"],
      "env": {}
    },
    "time": {
      "command": "npx",
      "args": ["mcp-time"],
      "env": {}
    },
    "git": {
      "command": "npx",
      "args": ["mcp-git"],
      "env": {}
    },
    "bash": {
      "command": "npx",
      "args": ["mcp-bash"]
    },
    "electron-cdp": {
      "command": "npx",
      "args": ["mcp-electron-cdp"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {}
    },
    "github-official": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer ${input:github_mcp_pat}"
      }
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-chrome-devtools"],
      "env": {}
    },
    "apple-docs": {
      "command": "npx",
      "args": ["-y", "@kimsungwhee/apple-docs-mcp"]
    },
    "applescript_execute": {
      "command": "npx",
      "args": [
        "@peakmojo/applescript-mcp"
      ]
    }
  }
}
