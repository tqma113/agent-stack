{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ],
      "env": {}
    },
    "fetch": {
      "command": "npx",
      "args": ["mcp-fetch"],
      "env": {}
    },
    "time": {
      "command": "npx",
      "args": ["mcp-time"],
      "env": {}
    },
    "git": {
      "command": "npx",
      "args": ["mcp-git"],
      "env": {}
    },
    "bash": {
      "command": "npx",
      "args": ["mcp-bash"]
    }
