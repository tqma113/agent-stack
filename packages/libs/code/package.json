{
  "name": "@ai-stack/code",
  "version": "0.0.1",
  "description": "Code Agent with file operations, search, git integration, and undo/redo capabilities",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "ai-code": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "code",
    "agent",
    "ai",
    "file-operations",
    "edit",
    "search",
    "git",
    "undo-redo"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-stack/agent": "workspace:*",
    "@ai-stack/knowledge": "workspace:*",
    "@ai-stack/mcp": "workspace:*",
    "@ai-stack/provider": "workspace:*",
    "@ai-stack/tui": "workspace:*",
    "better-sqlite3": "^11.7.0",
    "commander": "^12.1.0",
    "diff": "^7.0.0",
    "glob": "^11.0.0",
    "picomatch": "^4.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/diff": "^6.0.0",
    "@types/node": "^22.10.0",
    "@types/picomatch": "^3.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "~5.7.2"
  }
}
