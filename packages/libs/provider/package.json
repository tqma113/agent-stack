{
  "name": "@ai-stack/provider",
  "version": "0.0.1",
  "description": "Multi-model LLM provider for ai-stack monorepo (OpenAI, Anthropic, Google Gemini, and more)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agent",
    "provider",
    "openai",
    "anthropic",
    "claude",
    "gemini",
    "google",
    "ollama",
    "llm",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.77.0"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "~5.7.2"
  }
}
