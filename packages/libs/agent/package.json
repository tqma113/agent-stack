{
  "name": "@ai-stack/agent",
  "version": "0.0.1",
  "description": "Main entry point for ai-stack AI agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "ai-agent": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agent",
    "ai",
    "openai",
    "llm",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-stack/provider": "workspace:*",
    "@ai-stack/mcp": "workspace:*",
    "@ai-stack/skill": "workspace:*",
    "@ai-stack/memory": "workspace:*",
    "@ai-stack/memory-store-sqlite": "workspace:*",
    "@ai-stack/knowledge": "workspace:*",
    "@ai-stack/tui": "workspace:*",
    "commander": "^12.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "~5.7.2"
  }
}
